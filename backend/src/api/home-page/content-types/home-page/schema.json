        "sections.quote",
        "sections.cta",
        "sections.rechtsdomeinen-cta",
        "sections.image-article",
        "sections.kantoor",
        "sections.color-block-cta",
        "sections.vertical-list",
        "sections.double-header-paragraph",
        "text.ygwys"
      ]
