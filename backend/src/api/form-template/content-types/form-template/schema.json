{
  "kind": "collectionType",
  "collectionName": "form_templates",
  "info": {
    "singularName": "form-template",
    "pluralName": "form-templates",
    "displayName": "Form template",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "leftSection": {
      "type": "dynamiczone",
      "components": [
        "formfields.inputfield",
        "formfields.select-field",
        "formfields.text-area-field"
      ]
    },
    "rightSection": {
      "type": "dynamiczone",
      "components": [
        "formfields.inputfield",
        "formfields.select-field",
        "formfields.text-area-field"
      ]
    },
    "title_location": {
      "type": "enumeration",
      "enum": [
        "left",
        "right"
      ],
      "default": "left"
    },
    "form_intro": {
      "type": "text"
    },
    "submit_button_text": {
      "type": "string",
      "default": "Verzenden"
    },
    "thankYouSlug": {
      "type": "string"
    },
    "internalMailAddress": {
      "type": "string"
    },
    "internalMailId": {
      "type": "integer"
    },
    "FormId": {
      "type": "integer",
      "unique": true,
      "required": true
    },
    "mailId": {
      "type": "integer"
    }
  }
}
